{
  "moduleFileExtensions": [ "js",  "jsx" ],
  "collectCoverage": true,
  "coverageDirectory": "./coverage",
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "coveragePathIgnorePatterns": [
    "<rootDir>/node_modules",
    "<rootDir>/.*/__tests__"
  ]
}

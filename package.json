    "react-dom": "^15.4.2",
    "redux": "^3.6.0"
  },
  "dependencies": {
    "d3-hierarchy": "^1.0.3",
    "gl-matrix": "^2.3.2",
    "lodash.template": "^4.4.0",
    "opn": "^4.0.2",

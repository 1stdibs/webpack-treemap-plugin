{
  "name": "webpack-treemap",
  "version": "0.1.0",
  "description": "Treemap plugin for webpack builds",
  "author": "1stdibs Front-end Engineering",
  "private": false,
  "engines": {
    "node": ">=6.9.1"
  },
  "main": "plugin.js",
  "contributors": [
    {
      "name": "Walker Flynn",
      "email": "walker@1stdibs.com"
    }
  ],
  "scripts": {
    "clean": "rm -rf ./node_modules && yarn cache clean",
    "cleaninstall": "yarn run clean && yarn install",
    "test": "jest --config ./src/__tests__/jest.json",
    "lint": "dibslint --root=./src --es6",
    "precommit": "dibslint --git --warnings --es6 ",
    "prepublish": "yarn run compile && webpack",
    "compile": "rm -rf dist && babel src/ --out-dir dist --ignore ./**/__tests__"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
